let Map = {(0, "a"), (2, "r"), (5, "c")}

func get(M, k) =
    let answer = null
    for t ∈ M | t[0] = k do
        answer := t[1]
    answer

func add(M, k, v) = M ∪ {(k, v)}

func remove(M, k) = {t ∈ M | t[0] ≠ k}

// Examples
Map := add(Map, 6, "z")
println(get(Map, 0))
Map := remove(Map, 0)
println(Map)