with "math"

func is_prime(x) = 
    if x ≤ 1 then return false
    let i = 2
    while i ≤ floor(x / 2) do
        if x mod i == 0 then return false
        i := i + 1
    true

func n_primes(n) = 
    let P = {}
    let i = 0
    while i < n do
        if is_prime(i) then P := P ∪ {i}
        i := i + 1
    P

