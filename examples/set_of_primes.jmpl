with "math"

func is_prime(x) = 
    if x ≤ 1 then return false
    let i = 2
    while i ≤ floor(x / 2) do
        if x mod i == 0 then return false
        i := i + 1
    true

func n_primes(N) = {n ∈ {2 ... N} | is_prime(n)}

// or (slower)

func simple_n_primes(N) = {n ∈ {2 ... N} | ∀d ∈ {2, 3 ... floor(n / 2)} | n mod d ≠ 0}

