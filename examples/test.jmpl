let pi = 3.14159265359;

func sinOf(x) = out "Sin of " + x + " is " + sin(x) + ".";
func cosOf(x) = out "Cos of " + x + " is " + cos(x) + ".";
func tanOf(x) = out "Tan of " + x + " is " + tan(x) + ".";

sinOf(pi);
cosOf(pi);
tanOf(pi);

sinOf(pi / 2);
cosOf(pi / 2);
tanOf(pi / 2);

sinOf(0);
cosOf(0);
tanOf(0);