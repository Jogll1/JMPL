let a = 3
let b = (7 + 5)
let n = 5

out "a = " + a
out "b = " + b
out "n = " + n

// let z = (a * b); let answer = z mod n; out "(" + a + " * " + b + ") = " + z; out z + " mod " + n + " = " + answer;

func printTimesModN(x, y, n) = {
    let z = (x * y); let answer = z mod n; out "(" + x + " * " + y + ") = " + z; out z + " mod " + n + " = " + answer;
}

printTimesModN(a, b, n);

out "\nEnd Of Line"